# API Configuration - Staging URLs
API_BASE_URL=https://staging-api.fitnease.com
AUTH_SERVICE_URL=https://staging-auth.fitnease.com
CONTENT_SERVICE_URL=https://staging-content.fitnease.com
TRACKING_SERVICE_URL=https://staging-tracking.fitnease.com
PLANNING_SERVICE_URL=https://staging-planning.fitnease.com
SOCIAL_SERVICE_URL=https://staging-social.fitnease.com
ML_SERVICE_URL=https://staging-ml.fitnease.com
ENGAGEMENT_SERVICE_URL=https://staging-engagement.fitnease.com
COMMS_SERVICE_URL=https://staging-comms.fitnease.com
MEDIA_SERVICE_URL=https://staging-media.fitnease.com
OPS_SERVICE_URL=https://staging-ops.fitnease.com

# App Configuration
APP_NAME=FitNEase Staging
APP_VERSION=1.0.0
APP_ENVIRONMENT=staging
APP_BUNDLE_ID=com.fitnease.staging

# Features Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DEV_TOOLS=false

# Tabata Configuration
TABATA_WORK_DURATION=20
TABATA_REST_DURATION=10
TABATA_ROUNDS=8
TABATA_PREPARATION_TIME=10

# Storage Configuration
CACHE_TTL=7200
MAX_OFFLINE_REQUESTS=50

# Production
DEBUG_API_CALLS=false
LOG_LEVEL=info
ENABLE_FLIPPER=false