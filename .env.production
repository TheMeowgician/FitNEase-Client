# API Configuration - Production URLs
API_BASE_URL=https://api.fitnease.com
AUTH_SERVICE_URL=https://auth.fitnease.com
CONTENT_SERVICE_URL=https://content.fitnease.com
TRACKING_SERVICE_URL=https://tracking.fitnease.com
PLANNING_SERVICE_URL=https://planning.fitnease.com
SOCIAL_SERVICE_URL=https://social.fitnease.com
ML_SERVICE_URL=https://ml.fitnease.com
ENGAGEMENT_SERVICE_URL=https://engagement.fitnease.com
COMMS_SERVICE_URL=https://comms.fitnease.com
MEDIA_SERVICE_URL=https://media.fitnease.com
OPS_SERVICE_URL=https://ops.fitnease.com

# App Configuration
APP_NAME=FitNEase
APP_VERSION=1.0.0
APP_ENVIRONMENT=production
APP_BUNDLE_ID=com.fitnease.app

# Features Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DEV_TOOLS=false

# Tabata Configuration
TABATA_WORK_DURATION=20
TABATA_REST_DURATION=10
TABATA_ROUNDS=8
TABATA_PREPARATION_TIME=10

# Storage Configuration
CACHE_TTL=86400
MAX_OFFLINE_REQUESTS=25

# Production
DEBUG_API_CALLS=false
LOG_LEVEL=error
ENABLE_FLIPPER=false