# ==========================================
# üåê API CONFIGURATION
# ==========================================
# API URLs are now managed in: config/api.config.ts
# To change IP address, edit BASE_IP in that file
# ==========================================

# App Configuration
APP_NAME=FitNEase
APP_VERSION=1.0.0
APP_ENVIRONMENT=development
APP_BUNDLE_ID=com.fitnease.app

# Features Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_DEV_TOOLS=true

# Tabata Configuration
TABATA_WORK_DURATION=20
TABATA_REST_DURATION=10
TABATA_ROUNDS=8
TABATA_PREPARATION_TIME=10

# Storage Configuration
CACHE_TTL=3600
MAX_OFFLINE_REQUESTS=100
TOKEN_REFRESH_THRESHOLD=300

# Development
DEBUG_API_CALLS=true
LOG_LEVEL=debug
ENABLE_FLIPPER=true