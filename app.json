{
  "expo": {
    "name": "fitnease-client",
    "slug": "fitnease-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/FitNEase-App-Pic-logo.png",
    "scheme": "fitneaseclient",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "FitNEase needs camera access for video workouts with friends.",
        "NSMicrophoneUsageDescription": "FitNEase needs microphone access for video workouts with friends."
      }
    },
    "android": {
      "package": "com.fitnease.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/FitNEase-App-Pic-logo.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "VIBRATE",
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/FitNEase-App-Pic-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FitNEase to access your camera for video workouts.",
          "microphonePermission": "Allow FitNEase to access your microphone for video workouts."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e82fecda-c86d-4d66-a9af-7aba6056f28f"
      }
    },
    "owner": "gabsm2000"
  }
}
